        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="messageRestored" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Nachricht erfolgreich aus dem Papierkorb wiederhergestellt wurde. "false": wenn die Nachricht nicht erfolgreich aus dem Papierkorb wiederhergestellt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteMessageRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachrichten-Id der Nachricht welche gelöscht werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteMessageResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="messageDeleted" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Nachricht erfolgreich gelöscht wurde."false": wenn die Nachricht nicht erfolgreich gelöscht wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addFolderRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="folderName" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des neu zu erstellenden Ordners.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="parentFolderId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id des Ordners in dem der neue Ordner erstellt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addFolderResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="newFolder" type="beadto1:FolderSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der neu erstellte Ordner.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="updateFolderRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="folderId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id des Ordners welcher bearbeitet werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="newFolderName" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Name des zu bearbeitenden Ordners (falls dieser bearbeitet werden soll).</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="newParentFolderId" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Id des Ordners in dem der zu bearbeitende Ordner enthalten sein soll (falls dieser bearbeitet werden soll).</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="updateFolderResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="folderUpdated" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn der Ordner erfolgreich bearbeitet wurde. "false": wenn der Ordner nicht erfolgreich bearbeitet wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="removeFolderRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="folderId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id des Ordners welcher gelöscht werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="removeFolderResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="folderRemoved" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn der Ordner erfolgreich gelöscht wurde. "false": wenn der Ordner nicht erfolgreich gelöscht wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addCommentRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="comment" type="beadto2:CommentSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der zu erstellende Kommentar - die Kommentar-Id muss nicht gefüllt werden.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addCommentResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="newComment" type="beadto2:CommentSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der neu erstellte Kommentar - die Kommentar-Id wurde vom System gesetzt.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getMessageConfigRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getMessageConfigResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="messageConfig" type="beadto1:MessageConfigurationSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachrichten-Konfiguration.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="activateAuthenticationRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Postfachbesitzers für dessen Postfach ein neuer Sicherheits-Token freigeschaltet werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="securityToken" type="beadto1:SecurityTokenSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der freizuschaltende Sicherheits-Token.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="signedPrivilege" type="beadto1:SignedPrivilegeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das für das Sicherheits-Token und das Postfach signierte Recht für die Freischaltung.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="activateAuthenticationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="authenticationActivated" type="xsd:boolean"><xsd:annotation><xsd:documentation>"true": wenn der Sicherheits-Token erfolgreich freigeschaltet wurde. "false": wenn der Sicherheits-Token nicht erfolgreich freigeschaltet wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUnactivatedSecurityTokensRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Postfachbesitzers für dessen Postfach die noch nicht freigeschalteten Sicherheits-Token angefordert werden sollen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUnactivatedSecurityTokensResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="securityTokens" type="beadto1:SecurityTokenSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Die noch nicht freigeschalteten Sicherheits-Token des angeforderten Postfachs.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="version" type="xsd:int" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Version des zu erstellenden Signaturobjektes für die Rechtevergabe.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getAccessRightsRequest">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getAccessRightsResponse">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="accessRights" type="beadto5:AccessRightListSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Eine Liste von Berechtigungen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getAccessRightsGrantedRequest">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getAccessRightsGrantedResponse">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="accessRights" type="beadto5:AccessRightListSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Eine Liste von Berechtigungen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getMessageOverviewRequest">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
            	<xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachrichten-Id .</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="getMessageOverviewResponse">
        <xsd:complexType>
            <xsd:sequence>
            	<xsd:element name="messageOverview" type="beadto5:MessageOverviewSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation></xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>