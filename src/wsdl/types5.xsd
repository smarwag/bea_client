<?xml version="1.0" encoding="UTF-8"?>
	<xsd:schema
			xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:beadto1="http://brak.de/bea/application/dto/soap/dto1"
            xmlns:beadto2="http://brak.de/bea/application/dto/soap/dto2"
            xmlns:beadto3="http://brak.de/bea/application/dto/soap/dto3"
            xmlns:beadto4="http://brak.de/bea/application/dto/soap/dto4"
            xmlns:beadto5="http://brak.de/bea/application/dto/soap/dto5"
			attributeFormDefault="qualified"
			elementFormDefault="qualified"
			targetNamespace="http://brak.de/bea/application/dto/soap/types5">

    <xsd:import namespace="http://brak.de/bea/application/dto/soap/dto1" schemaLocation="dto1.xsd"/>
    <xsd:import namespace="http://brak.de/bea/application/dto/soap/dto2" schemaLocation="dto2.xsd"/>
    <xsd:import namespace="http://brak.de/bea/application/dto/soap/dto3" schemaLocation="dto3.xsd"/>
    <xsd:import namespace="http://brak.de/bea/application/dto/soap/dto4" schemaLocation="dto4.xsd"/>
    <xsd:import namespace="http://brak.de/bea/application/dto/soap/dto5" schemaLocation="dto5.xsd"/>

    <xsd:element name="createSessionKeyRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="createSessionKeyResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionKey" type="beadto5:SessionKeySoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getAddressbookRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getAddressbookResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="addressbook" type="beadto5:IdentityListSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Eine Liste aller Identitäten welche in dem Adressbuch des zur Session gehörigen Benutzers stehen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteAddressbookEntryRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="addressbookEntrySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Benutzers welchers aus dem Adressbuch gelöscht werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteAddressbookEntryResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="addressbookEntryDeleted" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn der Adressbuch-Eintrag gelöscht wurde. "false": wenn der Adressbuch-Eintrag nicht gelöscht wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAddressbookEntryRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="identitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Benutzers welcher dem Adressbuch hinzugefügt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAddressbookEntryResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="addressbookEntryAdded" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn der Adressbuch-Eintrag hinzugefügt wurde. "false": wenn der Adressbuch-Eintrag nicht hinzugefügt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getMessageRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachrichten-Id der zu holenden Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="markAsExport" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Nachricht als exportiert markiert werden soll. "false": wenn die Nachricht nicht als exportiert markiert werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getMessageResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="message" type="beadto5:MessageSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die verschlüsselte, angefragte Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="createNewMessageRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Postfachbesitzers für dessen Postfach eine Nachricht erstellt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="createNewMessageResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="message" type="beadto5:MessageSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die verschlüsselte, erstellte Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="loginUserRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die vorläufige Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="samlResponse" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das zu validierende SAML-Token.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="authCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Zertifikat welches zu Anmeldung verwendet werden soll (Sicherheits-Token).</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="loginUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionKey" type="beadto5:SessionKeySoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der verschlüsselte Session Key für die aktuelle Session.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="logoutUserRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der Session welche geschlossen werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="logoutUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="logoutSuccessful" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn das ausloggen erfolgreich war. "false": wenn das ausloggen nicht erfolgreich war.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="startLoginUserRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="authtificationType" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Authentifizierungs-Typ welcher für das Login benutzt werden soll.</xsd:documentation></xsd:annotation>
                	<xsd:simpleType>
						<xsd:restriction base="beadto1:AuthentificationTypeSoapDTO" />
					</xsd:simpleType> 
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="startLoginUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Eine vorläufige Id der Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="samlRequest" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der SAML Request welcher zur Authentifizierung benötigt wird.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="finishRegistrationRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>

                <xsd:element name="securityToken" type="beadto1:SecurityTokenSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Sicherheits-Token welches für den Benutzer hinterlegt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="signedPrivilege" type="beadto1:SignedPrivilegeSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Das signierte Recht für die Freischaltung des Sicherheits-Token auf dem Postfach.</xsd:documentation></xsd:annotation></xsd:element>

                <xsd:element name="securityQuestion" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Eine Sicherheitsfrage.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="securityAnswer" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Antwort zur ausgewählten Sicherheitsfrage.</xsd:documentation></xsd:annotation></xsd:element>

                <xsd:element name="email" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Email-Adresse des sich registrierenden Benutzers.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="finishRegistrationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="finishRegistrationCompleted" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Registrierung abgeschlossen ist. "false": wenn die Registrierung nicht erfolgreich war.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="startRegistrationRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice>
                    <xsd:element name="usernamePassword" type="beadto1:UsernamePasswordSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Registrierung eines Mitarbeiters: Die Benutzername/Passwort-Kombination des zu registrierenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                    <xsd:element name="samlResponse" type="beadto1:SAMLResponseSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Registrierung einer beA-Karte: Eine SAML-Response. </xsd:documentation></xsd:annotation></xsd:element>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="startRegistrationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="securityQuestionList" type="xsd:string" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Eine Liste von möglichen Sicherheitsfragen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAuthentificationRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="securityToken" type="beadto1:SecurityTokenSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Sicherheits-Token welcher dem Benutzer hinzugefügt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAuthentificationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="authentificationAdded" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn das hinzufügen des Sicherheits-Token erfolgreich war. "false": wenn das hinzufügen des Sicherheits-Token nicht erfolgreich war.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteAuthentificationRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="securityTokenName" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des zu löschenden Sicherheits-Token.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteAuthentificationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="authentificationDeleted" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn das löschen des Sicherheits-Token erfolgreich war. "false": wenn das löschen des Sicherheits-Token nicht erfolgreich war.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUserJournalResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="userJournals" type="beadto5:JournalListSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Journal-Einträge des Nutzerjournal.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUserJournalRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="identitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Benutzers für den das Nutzerjournal geholt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="from" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Filter: Datum ab wann Nutzerjournal-Einträge geholt werden sollen.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="until" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Filter: Datum bis wann Nutzerjournal-Einträge geholt werden sollen.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUserNameRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="identitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Benutzers für den der Benutzername geholt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getUserNameResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="username" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der angeforderte Benutzername.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addUserRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Postfachbesitzers für dessen Postfach ein Mitarbeiter erstellt werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="gender" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Geschlecht des anzulegenden Mitarbeiters.</xsd:documentation></xsd:annotation>
                	<xsd:simpleType>
						<xsd:restriction base="beadto1:GenderSoapTypeDTO" />
					</xsd:simpleType>  
                </xsd:element>
                <xsd:element name="title" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Titel des anzulegenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="firstname" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Vorname des anzulegenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="surname" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Nachname des anzulegenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="newIdentity" type="beadto5:IdentitySoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Identität des neu angelegten Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="username" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Benutzername des neu angelegten Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="password" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das initiale Passwort des neu angelegten Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="updateUserRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="identitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des zu bearbeitenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="gender" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Geschlecht des zu bearbeitenden Mitarbeiters (falls es bearbeitet werden soll).</xsd:documentation></xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="beadto1:GenderSoapTypeDTO" />
					</xsd:simpleType>                
                </xsd:element>
                <xsd:element name="title" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Titel des zu bearbeitenden Mitarbeiters (falls es bearbeitet werden soll).</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="firstname" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Vorname des zu bearbeitenden Mitarbeiters (falls es bearbeitet werden soll).</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="surname" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Nachname des zu bearbeitenden Mitarbeiters (falls es bearbeitet werden soll).</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="updateUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="userUpdated" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Bearbeitung des Mitarbeiters erfolgreich war. "false": wenn die Bearbeitung des Mitarbeiters nicht erfolgreich war.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteUserRequest">
        <xsd:complexType>	
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="recieverIdentitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des zu löschenden Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="deleteUserResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="userDeleted" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true: wenn der Mitarbeiter erfolgreich gelöscht wurde. "false": wenn der Mitarbeiter nicht gelöscht wurde.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAccessRightRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der zum aktuellen Login gehörigen Session.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Postfachbesitzers für dessen Postfach ein Recht vergeben werden soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="reciverIdentitySafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeId des Benutzers welcher ein Recht bekommen soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="privilege" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Das zu vergebene Recht.</xsd:documentation></xsd:annotation>
                	<xsd:simpleType>
						<xsd:restriction base="beadto5:PrivilegeTypeSoapDTO" />
					</xsd:simpleType>     
                </xsd:element>
                <xsd:element name="from" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Datum ab dem das Recht gültig sein soll.</xsd:documentation></xsd:annotation></xsd:element>
                <xsd:element name="until" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Datum bis wann ein Recht gültig sein soll.</xsd:documentation></xsd:annotation></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addAccessRightResponse">
        <xsd:complexType>
            <xsd:sequence>
