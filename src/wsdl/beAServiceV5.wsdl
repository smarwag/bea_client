	<wsdl:portType name="beAServiceV5PortType">
		<wsdl:operation name="getMessage">
			<wsdl:documentation>Holt eine Nachricht anhand ihrer Nachrichten-Id.</wsdl:documentation>
			<wsdl:input message="beatns:getMessageRequest"></wsdl:input>
			<wsdl:output message="beatns:getMessageResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="createNewMessage">
			<wsdl:documentation>Erstellt eine neue Nachricht für ein Postfach anhand der SafeId des Postfachbesitzers.</wsdl:documentation>
			<wsdl:input message="beatns:createNewMessageRequest"></wsdl:input>
			<wsdl:output message="beatns:createNewMessageResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="saveMessage">
			<wsdl:documentation>Speichert eine Nachricht.</wsdl:documentation>
			<wsdl:input message="beatns:saveMessageRequest"></wsdl:input>
			<wsdl:output message="beatns:saveMessageResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="sendMessage">
			<wsdl:documentation>Sendet eine Nachricht. Die Nachricht muss vorher gespeichert werden.</wsdl:documentation>
			<wsdl:input message="beatns:sendMessageRequest"></wsdl:input>
			<wsdl:output message="beatns:sendMessageResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="getFolderStructure">
			<wsdl:documentation>Holt die Ordnerstruktur eines Postfaches anhand der SafeId des Postfachbesitzers.</wsdl:documentation>
			<wsdl:input message="beatns:getFolderStructureRequest"></wsdl:input>
			<wsdl:output message="beatns:getFolderStructureResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="getFolderOverview">
			<wsdl:documentation>Holt die Nachrichtenübersicht eines Ordners anhand der Id des Ordners. Die Anzahl der zurückgelieferten Einträge ist aktuell auf 1000 begrenzt.</wsdl:documentation>
			<wsdl:input message="beatns:getFolderOverviewRequest"></wsdl:input>
			<wsdl:output message="beatns:getFolderOverviewResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="searchAddressee">
			<wsdl:documentation>Sucht einen Adressat anhand von mindestens zwei Suchparametern. Jeder Suchparameter muss aus mindestens zwei Zeichen bestehen.</wsdl:documentation>
			<wsdl:input message="beatns:searchAddresseeRequest"></wsdl:input>
			<wsdl:output message="beatns:searchAddresseeResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="getFavouriteAddressee">
			<wsdl:documentation>Holt eine Liste der favorisierten Adressaten des gerade eingeloggten Benutzers.</wsdl:documentation>
			<wsdl:input message="beatns:getFavouriteAddresseeRequest"></wsdl:input>
			<wsdl:output message="beatns:getFavouriteAddresseeResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="deleteFavouriteAddressee">
			<wsdl:documentation>Löscht einen favorisierten Adressaten (des gerade eingeloggten Benutzers) anhand der SafeId des Adressaten.</wsdl:documentation>
			<wsdl:input message="beatns:deleteFavouriteAddresseeRequest"></wsdl:input>
			<wsdl:output message="beatns:deleteFavouriteAddresseeResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="moveMessageToFolder">
			<wsdl:documentation>Verschiebt eine Nachricht in einen Ordner anhand der Nachrichten-Id und der Id des Ordners in den die Nachricht verschoben werden soll. Ursprungs- und Zielordner müssen in demselben Postfach liegen.</wsdl:documentation>
			<wsdl:input message="beatns:moveMessageToFolderRequest"></wsdl:input>
			<wsdl:output message="beatns:moveMessageToFolderResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

		<wsdl:operation name="moveMessageToTrash">
			<wsdl:documentation>Verschiebt eine Nachricht (anhand der Nachrichten-Id) in den Papierkorb.</wsdl:documentation>
			<wsdl:input message="beatns:moveMessageToTrashRequest"></wsdl:input>
			<wsdl:output message="beatns:moveMessageToTrashResponse"></wsdl:output>
			<wsdl:fault name="BeaFault" message="beatns:BeaFault"></wsdl:fault>
		</wsdl:operation>

