            <xsd:enumeration value="PRV_VERWENDEN_SYSTEMJOURNAL"><xsd:annotation><xsd:documentation>Recht: Verwenden des Systemjournals</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_ERSTELLUNG_BERICHTE_FUER_EINE_RAK"><xsd:annotation><xsd:documentation>Recht: Berichte erstellen (RAK)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_ERSTLLUNG_BERICHTE_FUER_DIE_BEA_ANWENDUNG"><xsd:annotation><xsd:documentation>Recht: Berichte erstellen (beA)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_POSTFAECHER_AUFLISTEN"><xsd:annotation><xsd:documentation>Recht: Postfächer auflisten</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_POSTFACH_DETAILANSICHT"><xsd:annotation><xsd:documentation>Recht: Postfachdetails öffnen</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_VERWENDUNG_NUTZERJOURNAL"><xsd:annotation><xsd:documentation>Recht: Verwenden Nutzerjournal</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_BENUTZER_ANZEIGEN"><xsd:annotation><xsd:documentation>Recht: Benutzer anzeigen</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_VERWENDUNG_ADMINISTRATION"><xsd:annotation><xsd:documentation>Recht: Verwaltung von Benutzern</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_VERWENDUNG_POSTFACHJOURNAL_NACHRICHTENJOURNAL"><xsd:annotation><xsd:documentation>Recht: Postfach- und Nachrichtenjournal verwenden</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_BENUTZER_ZURUECKSETZEN"><xsd:annotation><xsd:documentation>Recht: Benutzer zurücksetzen</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PRV_SECURITY_TOKEN_LOESCHEN"><xsd:annotation><xsd:documentation>Recht: Sicherheits Token löschen</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="PrivilegeListSoapDTO">
        <xsd:sequence>
            <xsd:element name="privilege" type="beadto1:PrivilegeTypeSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="JournalListSoapDTO">
        <xsd:sequence>
            <xsd:element name="journals" type="beadto1:JournalSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Eine Liste an Journal-Einträgen.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="JournalSoapDTO">
        <xsd:sequence>
            <xsd:element name="journalType" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Typ des Journals (Nachrichtenjournal, Postfachjournal, Benutzerjournal oder Systemjournal).</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="eventType" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Typ des Events welches den Journal-Eintrag ausgelöst hat.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Wenn es sich um ein Nachrichtenjournal-Eintrag handelt: die Id der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="attachmentReference" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Wenn es sich um ein Nachrichtenjournal-Eintrag handelt: der Dateiname des angehängten Anhangs.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Wenn es sich um ein Nachrichtenjournal-Eintrag handelt: die SafeId des Postfachs in der die Nachricht liegt welche den Journal-Eintrag ausgelöst hat. Wenn es sich um ein Postfachjournal-Eintrag handelt: die SafeID des Postfachs welche den Journal-Eintrag ausgelöst hat.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sourceIdentity" type="beadto1:IdentitySoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Identität die den Journal-Eintrag ausgelöst hat.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="targetIdentity" type="beadto1:IdentitySoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Identität für die der Journal-Eintrag erstellt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceIdentity" type="beadto1:IdentitySoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="timestamp" type="xsd:date" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Zeitstempel des Journal-Eintrags.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="AuthentConfigurationSoapDTO">
        <xsd:sequence>
            <xsd:element name="address" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Adresse des Authentifizierungs-Servers (SAFE-BRAK).</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="provider" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des Diensteanbieters für die Anmeldung.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sigAlgorithm" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des Signatur-Algorithmus.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="receiverServlet" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die URL des Receiver Servlets.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="untrustedCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das SSL-Zertifikat eines Testservers(nur für Testbetrieb).</xsd:documentation></xsd:annotation></xsd:element>					
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PeriodSoapDTO">
        <xsd:sequence>
            <xsd:element name="from" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="to" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageListSoapDTO">
        <xsd:sequence>
            <xsd:element name="messagesSoapDTO" type="beadto1:MessagesSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessagesSoapDTO">
        <xsd:sequence>
            <xsd:element name="message" type="beadto1:MessageSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="messageInfo" type="beadto1:MessageInfoSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProcessCardSoapDTO">
        <xsd:sequence>
            <xsd:element name="processCardId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id des Laufzettels.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der Nachricht zu welcher der Laufzettel gehört.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="conversationId" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Conversation-Id aus dem entschlüsselten Laufzettel.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="type" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Typ aus dem entschlüsselten Laufzettel.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="challange" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Challenge-Text aus dem entschlüsselten Laufzettel.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="data" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die verschlüsselten Daten des Laufzettels.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="encKey" type="beadto1:EncryptedDataSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der verschlüsselte Schlüssel für die entschlüsselung des Laufzettels.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="processCard" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der entschlüsselte Laufzettel als XML.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProcessCardsSoapDTO">
        <xsd:sequence>
            <xsd:element name="processcards" type="beadto1:ProcessCardSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Eine Liste an Laufzetteln.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="VerificationResultStatusSoapDTO">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="SUCCESS"><xsd:annotation><xsd:documentation>Status des Prüfprotokolls: Erfolgreich</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PARTIAL"><xsd:annotation><xsd:documentation>Status des Prüfprotokolls: Teilweise erfolgreich</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="FAILED"><xsd:annotation><xsd:documentation>Status des Prüfprotokolls: Fehlgeschlagen</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="VerificationResultSoapDTO">
        <xsd:sequence>
            <xsd:element name="messageId" type="xsd:long"><xsd:annotation><xsd:documentation>Die Id der Nachricht zu der das Prüfprotokoll gehört.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="html" type="xsd:string"><xsd:annotation><xsd:documentation>HTML Repräsentation des Prüfprotokolls.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="xml" type="xsd:string"><xsd:annotation><xsd:documentation>XML Repräsentation des Prüfprotokolls.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="status" type="beadto1:VerificationResultStatusSoapDTO"><xsd:annotation><xsd:documentation>Status des Prüfprotokolls</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="VerificationConfigSoapDTO">
        <xsd:sequence>
            <xsd:element name="serverURL" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die URL zum OCSP Relay.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="serverSignatureCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das SSL Zertifikat des OCSP Relay.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageConfigurationSoapDTO">
        <xsd:sequence>
            <xsd:element name="maxAttachmentSizeInKB" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die maximale Größe eines Anhangs in KB.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="maxAttachmentCount" type="xsd:int" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die maximale Anzahl an Anhängen.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="attachmentExtensionBlacklist" type="xsd:string" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Eine Liste der verbotenen Dateiendungen eines Anhangs.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
