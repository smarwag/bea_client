<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
			xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:beadto1="http://brak.de/bea/application/dto/soap/dto1"
			attributeFormDefault="qualified"
			elementFormDefault="qualified"
			targetNamespace="http://brak.de/bea/application/dto/soap/dto1">

    <xsd:complexType name="BeAFaultSoapDTO">
        <xsd:sequence>
            <xsd:element name="faultId" type="xsd:long"></xsd:element>
            <xsd:element name="faultCode" type="xsd:string"></xsd:element>
            <xsd:element name="faultMessage" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageSoapDTO">
        <xsd:sequence>
            <xsd:element name="osciMessageId" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die OSCI Nachrichten-Id.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="osciSubject" type="beadto1:OSCISubjectTypeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der OSCI Betreff der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="attachments" type="beadto1:AttachmentSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Die Anhänge der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="encryptedObject" type="beadto1:EncryptedObjectSoapDTO" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Die verschlüsselten Bestandteile einer Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="metaData" type="beadto1:MetaDataSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Meta-Daten der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="newEGVPMessage" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Flag für neu eigegangene EGVP-Nachricht (noch kein Laufzettel entschlüsselt).</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="certificateMap" type="beadto1:CertificateMapSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Eine Zertifikats-Map für jedes in der Nachricht vorkommende Postfach.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="version" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Versionsnummer der Nachricht in der Datenbank.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="SecurityTokenTypeSoapDTO">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="HW"><xsd:annotation><xsd:documentation>Typ des Sicherheits-Token: Hardware Sicherheits-Token</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SW"><xsd:annotation><xsd:documentation>Typ des Sicherheits-Token: Software Sicherheits-Token</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="SecurityTokenSoapDTO">
        <xsd:sequence>
            <xsd:element name="tokenId" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Id des Sicherheits-Token.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="tokenName" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des Sicherheits-Token.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="tokenType" type="beadto1:SecurityTokenTypeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Typ des Sicherheits-Token.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="authCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Authentifizierungs-Zertifikat des Benutzers.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="cryptoCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Verschlüsselungs-Zertifikat des Benutzers.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="SignedPrivilegeTypeSoapDTO">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="CRYPTO"><xsd:annotation><xsd:documentation>Typ des signierten Rechts: Verschlüsselung</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="AUTH"><xsd:annotation><xsd:documentation>Typ des signierten Rechts: Authentifizierung</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="SignedPrivilegeSoapDTO">
        <xsd:sequence>
            <xsd:element name="privilegeType" type="beadto1:SignedPrivilegeTypeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Typ des signierten Rechts.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des Postfachs für das das signierte Recht ausgestellt ist.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="signature" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das signierte Recht.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UsernamePasswordSoapDTO">
        <xsd:sequence>
            <xsd:element name="username" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Benutzername des neuen Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="password" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Passwort des neuen Mitarbeiters.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SAMLResponseSoapDTO">
        <xsd:sequence>
            <xsd:element name="sessionId" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Id der Session für die der SAML-Response erstellt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="samlResponse" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SAML-Response.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="authCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Authentifizierungs-Zertifikat.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CertificateMapSoapDTO">
        <xsd:sequence>
            <xsd:element name="certificateEntry" type="beadto1:CertificateEntrySoapDTO" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Ein Zertifikatseintrag für ein in der Nachricht vorkommendes Postfach.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CertificateEntrySoapDTO">
        <xsd:sequence>
            <xsd:element name="reference" type="xsd:string"><xsd:annotation><xsd:documentation>Die SafeID des Postfachs zu dem das Zertifikat gehört.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="certificate" type="xsd:base64Binary"><xsd:annotation><xsd:documentation>Das Zertifikat in base64 kodierter Binärdarstellung.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="OSCISubjectTypeSoapDTO">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ALLGEMEINE_NACHRICHT"><xsd:annotation><xsd:documentation>OSCISubjectType: Allgemeine Nachricht</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HR_BETEILIGTER"><xsd:annotation><xsd:documentation>OSCISubjectType: HR-Beteiligter</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HR_GERICHT"><xsd:annotation><xsd:documentation>OSCISubjectType: HR-Gericht</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="MAHN_ANTRAG"><xsd:annotation><xsd:documentation>OSCISubjectType: Mahn-Antrag</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="MAHN_NACHRICHT"><xsd:annotation><xsd:documentation>OSCISubjectType: Mahn-Nachricht</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="MAHN_ABGABE"><xsd:annotation><xsd:documentation>OSCISubjectType: Mahn-Abgabe</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="TESTNACHRICHT"><xsd:annotation><xsd:documentation>OSCISubjectType: Testnachricht</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="UBEST_MAHN"><xsd:annotation><xsd:documentation>OSCISubjectType: Ubest-Mahn</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AttachmentSoapDTO">
        <xsd:sequence>
            <xsd:element name="reference" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Referenz auf die angehängte Datei: Dateiname</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="alias" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Alias des Anhangs.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="data" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der verschlüsselte Anhang in base64 kodierter Binärdarstellung.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="key" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der verschlüsselte Nachrichtenschlüssel.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="type" type="beadto1:AttachmentTypeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Typ des Anhangs.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sizeKB" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Größe des (unverschlüsselten) Anhangs in KB.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sizeEncryptedKB" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Größe des (verschlüsselten) Anhangs in KB.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="hashValue" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Hash-Wert (SHA256) des Anhangs.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="AttachmentTypeSoapDTO">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="SCHRIFTSATZ"><xsd:annotation><xsd:documentation>Typ des Anhangs: Schriftsatz</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="ATTACHMENT"><xsd:annotation><xsd:documentation>Typ des Anhangs: Anlage</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="EB"><xsd:annotation><xsd:documentation>Typ des Anhangs: EB</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="EMPTY"><xsd:annotation><xsd:documentation>Typ des Anhangs: -leer-</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="EncryptedObjectSoapDTO">
        <xsd:sequence>
            <xsd:element name="enc_data" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die verschlüsselten Daten des Objekts.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="enc_name" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des verschlüsselten Objekts.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="encKeyInfo" type="beadto1:EncKeyInfoSoapDTO" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Die Schlüsselinformationen zum verschlüsselten Objekt.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EncKeyInfoSoapDTO">
        <xsd:sequence>
            <xsd:element name="encKey" type="beadto1:EncryptedDataSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Daten-Objekt zu den Schlüsselinformationen.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="encCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Verschlüsselungs-Zertifikat des Postfachs.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="safeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des Postfachs.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EncryptedDataSoapDTO">
        <xsd:sequence>
            <xsd:element name="iv" type="xsd:base64Binary" minOccurs="1" maxOccurs="1"><xsd:annotation><xsd:documentation>Der Initialisierungsvektor, der bei der Umschlüsselung verwendet wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="value" type="xsd:base64Binary" minOccurs="1" maxOccurs="1"><xsd:annotation><xsd:documentation>Das Kryptogramm des umgeschlüsselten Nachrichtenschlüssels.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="tag" type="xsd:base64Binary" minOccurs="1" maxOccurs="1"><xsd:annotation><xsd:documentation>Das Authentifizierungstag des umgeschlüsselten Nachrichtenschlüssels (dient zur Integritätsprüfung).</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MessageInfoSoapDTO">
        <xsd:sequence>
            <xsd:element name="messageId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sender" type="beadto1:IdentitySoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Absender der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceNumber" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen Sender.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceNumberOpposition" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen der Gegenseite.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceNumberJustice" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen Empfänger.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="created" type="xsd:dateTime" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Zeitpunkt an dem die Nachricht erstellt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sent" type="xsd:dateTime" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Zeitpunkt an dem die Nachricht versandt wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="received" type="xsd:dateTime" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Zeitpunkt an dem die Nachricht empfangen wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="appointment" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="urgent" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Dringend".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="checkRequired" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Zu prüfen".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="confidential" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Persönlich/vertraulich".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="folderId" type="xsd:long" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Id des Ordners in dem die Nachricht liegt.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="addressees" type="beadto1:IdentityListSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Empfänger der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="attributes" type="beadto1:AttributeSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="comments" type="beadto1:CommentSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Die Kommentare der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="labels" type="beadto1:LabelSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="attachments" type="beadto1:AttachmentSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Die unverschlüsselten Informationen über die Anhänge der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="readFlags" type="beadto1:IdentityListSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Eine Liste der Benutzer welche die Nachricht bereits gelesen haben.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RecipientSoapDTO">
        <xsd:sequence>
            <xsd:element name="safeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des Absenders/Empfängers.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des Absenders/Empfängers.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="certificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Das Empfänger-Zertifikat.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MetaDataSoapDTO">
        <xsd:sequence>
            <xsd:element name="receptionTime" type="xsd:dateTime" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Zeitpunkt zu dem die Nachricht empfangen wurde.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="sender" type="beadto1:RecipientSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Absender der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="addressee" type="beadto1:RecipientSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Der/Die Empfänger der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="subject" type="beadto1:EncryptedDataSoapDTO" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Der Betreff der Nachricht.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceNumber" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen Sender.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceNumberOpposition" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen der Gegenseite.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="referenceJustice" type="xsd:string" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Aktenzeichen Empfänger.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="messageSigned" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn die Nachricht signiert ist. "false": wenn die Nachricht nicht signiert ist.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="oneAttachmentSigned" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>"true": wenn mind. ein Anhang signiert ist. "false": wenn kein Anhang signiert ist.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="urgent" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Dringend".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="checkRequired" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Zu prüfen".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="confidential" type="xsd:boolean" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die Nachricht gilt als "Persönlich/vertraulich".</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="appointment" type="xsd:date" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>deprecated</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="originatorCertificate" type="xsd:base64Binary" maxOccurs="1" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SessionKeySoapDTO">
        <xsd:sequence>
            <xsd:element name="sessionKey" type="xsd:base64Binary" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der verschlüsselte Session Key für die aktuelle Session.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="encrypteddata" type="beadto1:EncryptedDataSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Ein verschlüsselter Wert für die Überprüfung des ausgestellten Session-Keys.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="safeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des gerade angemeldeten Benutzers.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PostboxesSoapDTO">
        <xsd:sequence>
            <xsd:element name="postbox" type="beadto1:PostboxSoapDTO" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Alle Postfächer auf denen der eingeloggte Benutzer das Recht "Nachrichtenübersicht öffnen" besitzt.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PostboxSoapDTO">
        <xsd:sequence>
            <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des Postfachs.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="folder" type="beadto1:FolderSoapDTO" maxOccurs="unbounded" minOccurs="1"><xsd:annotation><xsd:documentation>Alle Ordner die zu diesem Postfach gehören.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="label" type="beadto1:LabelSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Alle Etiketten die zu diesem Postfach gehören.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="FolderSoapDTO">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Id des Ordners.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="type" type="beadto1:FolderTypeSoapDTO" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Ordnertyp des Ordners.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Der Name des Ordners.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="postboxSafeId" type="xsd:string" maxOccurs="1" minOccurs="1"><xsd:annotation><xsd:documentation>Die SafeID des Postfachs zu dem dieser Ordner gehört.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="parentFolderId" type="xsd:long" maxOccurs="1" minOccurs="0"><xsd:annotation><xsd:documentation>Die Id des Ordners in dem dieser Ordner liegt.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="childFolder" type="beadto1:FolderSoapDTO" maxOccurs="unbounded" minOccurs="0"><xsd:annotation><xsd:documentation>Die Ids der Ordner die in diesem Ordner liegen.</xsd:documentation></xsd:annotation></xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LabelSoapDTO">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:long"><xsd:annotation><xsd:documentation>Die Id des Etiketts.</xsd:documentation></xsd:annotation></xsd:element>
            <xsd:element name="name" type="xsd:string"><xsd:annotation><xsd:documentation>Der Name des Etiketts.</xsd:documentation></xsd:annotation></xsd:element>
